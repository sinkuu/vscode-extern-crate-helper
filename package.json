{
  "name": "extern-crate-helper",
  "displayName": "extern crate helper",
  "description": "Suggests you install missing dependencies to Cargo.toml",
  "version": "0.0.1",
  "publisher": "sinkuu",
  "license": "MIT",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:rust",
    "onLanguage:toml"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.sayHello",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "@types/node": "^6.0.40"
  },
  "dependencies": {
    "toml": "^2.3.1"
  }
}
